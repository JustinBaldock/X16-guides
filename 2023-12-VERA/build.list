002916  1               initVariables:
002916  1                   ; set start background color to 0
002916  1  A9 00            lda #0
002918  1  85 25            sta GAME_BACKGROUND
00291A  1                   ; re-start the clock
00291A  1  20 DB FF         jsr KERNAL_SETTIM
00291D  1                   ; setup our timing
00291D  1  20 DE FF         jsr KERNAL_RDTIM ; Call kernal function to get time
002920  1  85 24            sta GAME_TIMER ; store in our variable
002922  1                   ; clear screen and set text to white
002922  1  A9 93            lda #CTRL_CLR
002924  1  20 D2 FF         jsr KERNAL_CHROUT
002927  1  A9 05            lda #COLOUR_WHITE
002929  1  20 D2 FF         jsr KERNAL_CHROUT
00292C  1                   ; Define Sprite
00292C  1  9C 25 9F         stz VERA_CTRL
00292F  1  A9 10            lda #$10
002931  1  09 00            ora #<SPRITE_GRAPHICS
002933  1  8D 22 9F         sta VERA_ADDR_H
002936  1  A9 40            lda #>SPRITE_GRAPHICS
002938  1  8D 22 9F         sta VERA_ADDR_H
00293B  1  9C 20 9F         stz VERA_ADDR_L
00293E  1                   ; load sprite
00293E  1  A9 10            lda #<data                          ; Prepare for data copy from RAM to VRAM
002940  1  85 26            sta REGISTER_COPY
002942  1  A9 09            lda #>data
002944  1  85 27            sta REGISTER_COPY+1
002946  1  A2 20            ldx #32
002948  1  A0 00            ldy #0
00294A  1               @copyLoop:
00294A  1  B1 26            lda (REGISTER_COPY),y
