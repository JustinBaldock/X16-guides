002994  1               ; ----------
002994  1               ; function - wait for timer interrupt
002994  1               ; ----------
002994  1               ; This function uses the variable GAME_TIMER
002994  1               ; The variable GAME_TIMER should be initialized during program start
002994  1               ; Using the function KERNAL_RDTIM and saving reg A into GAME_TIMER
002994  1               waitTimerInterrupt:
002994  1               @start:
002994  1                   ; none
002994  1               @loop:
002994  1  20 DE FF         jsr KERNAL_RDTIM
002997  1  C5 24            cmp GAME_TIMER ; Compare current jiffy in A to previous jiffy in GAME_TIMER
002999  1  F0 F9            beq @loop ; If not different then loop
