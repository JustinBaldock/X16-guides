0028B5  1               bounceSprite:
0028B5  1                   ; update x
0028B5  1  AD 10 08         lda sprite1PosX
0028B8  1  18               clc
0028B9  1  6D 15 08         adc sprite1DeltaX
0028BC  1  8D 10 08         sta sprite1PosX
0028BF  1  AD 11 08         lda sprite1PosX+1
0028C2  1  6D 16 08         adc sprite1DeltaX+1
0028C5  1  8D 11 08         sta sprite1PosX+1
0028C8  1                   ; update y
0028C8  1  AD 12 08         lda sprite1PosY
0028CB  1  18               clc
0028CC  1  6D 17 08         adc sprite1DeltaY
0028CF  1  8D 12 08         sta sprite1PosY
0028D2  1  AD 13 08         lda sprite1PosY+1
0028D5  1  6D 18 08         adc sprite1DeltaY+1
0028D8  1  8D 13 08         sta sprite1PosY+1
0028DB  1                   ; check right bounds
0028DB  1               @checkRight:
0028DB  1  AD 11 08         lda sprite1PosX+1
0028DE  1  C9 01            cmp #$01
0028E0  1  D0 12            bne @checkLeft
0028E2  1  AD 10 08         lda sprite1PosX
0028E5  1  C9 1F            cmp #$1F
0028E7  1  D0 0B            bne @checkLeft
0028E9  1                   ; hit the right edge, change delta x = -1
0028E9  1  A9 FF            lda #255
0028EB  1  8D 15 08         sta sprite1DeltaX
0028EE  1  8D 16 08         sta sprite1DeltaX+1
0028F1  1  4C 06 29         jmp @checkBottom
0028F4  1               @checkLeft:
0028F4  1  AD 11 08         lda sprite1PosX+1
0028F7  1  D0 0D            bne @checkBottom
0028F9  1  AD 10 08         lda sprite1PosX
0028FC  1  D0 08            bne @checkBottom
0028FE  1                   ; hit the left edge, change delta x <= +1
0028FE  1  A9 01            lda #1
002900  1  8D 15 08         sta sprite1DeltaX
002903  1  9C 16 08         stz sprite1DeltaX+1
002906  1               @checkBottom:
002906  1  AD 13 08         lda sprite1PosY+1
002909  1  C9 00            cmp #$00
00290B  1  D0 12            bne @checkTop
00290D  1  AD 12 08         lda sprite1PosY
002910  1  C9 CF            cmp #$cf
002912  1  D0 0B            bne @checkTop
002914  1                   ; hit bottom edge, delta y <= -1
